#Formatação de Strings com várias linhas
escola = 'SENAI'
curso = 'Desenvolvimento de Sistemas'
uc = 'Lógica de Programação'
matricula = 34
nota = 8.999999
print(f"Escola: {escola}\n"
      f"Curso: {curso}\n"
      f"Unidade Curricular: {uc}\n"
      f"Matrícula {matricula:06}\n"
      f"Nota: {nota: .2f}")
